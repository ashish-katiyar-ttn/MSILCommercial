<script type="text/javascript">
    $(document).ready(function () {
        $("#btnSearch").click(function () {
            var Id = "test";
            if (Id != "") {
                $.ajax({
                    url: "http://msilcommercial.local:82/api/sitecore/Main/GetDealerDetails",
                    type: "POST",
                    data: { stateId: Id },
                    context: this,
                    success: function (data) {
                        alert("sucesss");
                        console.log("success", data);
                    },
                    error: function (data) {
                        console.log("error", data);
                    }
                });
            }
            else {
                $("#btnSearch").text("");
            }

        });
    });
</script>

